{
	"name": "sample-react-ssr",
	"private": true,
	"version": "1.0.0",
	"description": "Sample application to React Server-Side Rendering.",
	"scripts": {
		"check": "tsc --noEmit",
		"build": "cross-env NODE_ENV=production ENVIRONMENT=production tsc -p tsconfig.build.json",
		"start": "cross-env NODE_ENV=development npm-run-all --parallel dev:*",
		"dev:build": "nodemon --watch 'build/**/*.js' --exec 'node' ./build/bundle.js",
		"dev:build-server": "cross-env NODE_ENV=development webpack --config webpack.server.config.js --watch",
		"dev:build-client": "cross-env NODE_ENV=development webpack --config webpack.client.config.js --watch",
		"launch": "cross-env NODE_ENV=production node ./build/index.js",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint --ext .ts --ignore-path .gitignore --fix src",
		"test": "cross-env NODE_ENV=test jest",
		"test:debug": "cross-env NODE_ENV=test node --inspect ./node_modules/.bin/jest --runInBand"
	},
	"author": "caiquearaujo",
	"license": "MIT",
	"homepage": "https://github.com/caiquearaujo/sample-microservice-fastify-blog-posts#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/caiquearaujo/sample-microservice-fastify-blog-posts.git"
	},
	"bugs": {
		"url": "https://github.com/caiquearaujo/sample-microservice-fastify-blog-posts/issues"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-object-rest-spread": "^7.20.7",
		"@babel/plugin-transform-modules-commonjs": "^7.20.11",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"@types/jest": "^29.2.5",
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.48.0",
		"@typescript-eslint/parser": "^5.48.0",
		"@zerollup/ts-transform-paths": "^1.7.18",
		"autoprefixer": "^10.4.13",
		"babel-core": "^6.26.3",
		"babel-loader": "^9.1.2",
		"eslint": "^8.31.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-import-resolver-typescript": "^3.5.2",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.3",
		"jest": "^29.3.1",
		"nodemon": "^2.0.20",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^10.1.0",
		"postcss-import": "^15.1.0",
		"postcss-nested": "^6.0.0",
		"postcss-preset-env": "^8.0.1",
		"postcss-scss": "^4.0.6",
		"prettier": "^2.8.1",
		"sass": "^1.57.1",
		"sass-loader": "^13.2.0",
		"stylelint": "^14.16.1",
		"stylelint-config-standard-scss": "^6.1.0",
		"stylelint-prettier": "^2.0.0",
		"ts-jest": "^29.0.3",
